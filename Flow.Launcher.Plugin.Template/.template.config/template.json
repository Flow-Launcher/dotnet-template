{
    "$schema": "http://json.schemastore.org/template",
    "author": "Ioannis G.",
    "classifications": [ "Library", "Plugin", "Flow-Launcher" ],
    "name": "Plugin for the Flow launcher",
    "identity": "Flow.Launcher.Plugin",
    "shortName": "flow-plugin",
    "tags": {
      "language": "C#",
      "type":"project"
    },
    "sourceName": "MyFlowPlugin",
    "preferNameDirectory": false,
    "symbols":{
      "pluginId": {
        "type": "generated",
        "generator": "guid",
        "replaces":"PluginId",
        "parameters": {
            "format":"N"
        }
      },
      "pluginAuthor": {
        "type": "parameter",
        "defaultValue": "AuthorName",
        "replaces":"PluginAuthor"
      },
      "keyword": {
        "type": "parameter",
        "defaultValue": "*",
        "replaces":"PluginActionKeyword"
      },
      "description": {
        "type": "parameter",
        "defaultValue": "a new plugin for Flow",
        "replaces":"PluginDescription"
      }
    }
}